\begin{frame}{Le mod\`ele na\"if} 


\begin{algorithmic}
\Function{AgentBehavior}{ }
    \Loop
    \If{$FedUp()$} 
    \State $ChooseNewGoal()$
    \EndIf
    \State $FaceGoal()$
    \State $LookAhead()$
    \While{$getProba(patchAhead) < 1$}
    \State $AvoidPatchAhead()$
    \State $LookAhead()$
    \EndWhile
    \State $GoAhead()$ 
    \EndLoop
\EndFunction

\end{algorithmic}

 \end{frame}

