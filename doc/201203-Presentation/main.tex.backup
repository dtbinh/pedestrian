 \begin{frame}{Le mod\`ele na\"if} 
 \begin{algorithm}
\caption{Behavior of each agents}
\label{alg1}
\begin{algorithmic}
\While {TRUE}
  \If{FedUp} 
	\State choose new-goal
  \EndIf
  \State FaceGoal
  \State LookAhead
  \While{not Possible}
    \State avoidFrontPatch
    \State LookAhead
  \EndWhile
  \State MoveOf 1 
\EndWhile

\end{algorithmic}
\end{algorithm}

  
